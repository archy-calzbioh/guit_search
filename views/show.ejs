    <title>Document</title>
<table>
    <tr>
        <td><img id="viewImg" class="animate__bounceInLeft" src="<%=guitar.img%>"></td>
    </tr>
    <% for(var prop in guitar) { %>
<tr>
    <% if (prop != "img" && prop != "_id" && prop != "__v") { %>
    <td><%= prop %></td>
    <td><%=guitar[prop]%></td>
    <% } %>
</tr>
    <%}%>
</table>
  <a href="/add-to-wishlist/<%= guitar._id %>" class="btn btn-primary">Add to Wishlist</a>
<a href="/" class="btn btn-primary">Back</a>
</html>

