<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs') %>
  <body>
    <h1>Wishlist</h1>
    <header><%- include('./partials/header.ejs') %></header>
    <% wishlist.forEach(function(item) { %>
    <div>
      <h2><%= item.name %></h2>
      <p>Price: $<%= item.price %></p>
      <p>Quantity: <%= item.quantity %></p>
      <% guitars.forEach(function(guitar) { %> <% if (guitar._id.toString() ===
      item.id) { %>
      <img src="<%= guitar.img %>" width="200" />
      <p><%= guitar.make %> <%= guitar.model %></p>
      <% } %> <% }); %>
    </div>
    <% }); %>
    <a href="/empty-wishlist" class="btn btn-primary">Empty Wishlist</a>
    <a href="/" class="btn btn-primary">Back to Home</a>
  </body>
</html>
